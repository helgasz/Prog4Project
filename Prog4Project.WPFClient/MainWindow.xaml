<Window x:Class="Prog4Project.WPFClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prog4Project.WPFClient"
        mc:Ignorable="d"
        Title="Company Project Manager" Height="450" Width="800">

    <Grid>
        <StackPanel>
            
            <Button Click="WorkerEditor" Padding="3" Margin="5" FontSize="14">Worker</Button>
            <Button Click="ManagerEditor" Padding="3" Margin="5" FontSize="14">Manager</Button>
            <Button Click="PositionEditor" Padding="3" Margin="5" FontSize="14">Position</Button>
            <Button Click="ProjectEditor" Padding="3" Margin="5" FontSize="14">Project</Button>
        </StackPanel>
    </Grid>
</Window>
